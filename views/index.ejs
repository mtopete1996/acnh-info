<!DOCTYPE html>
<html>
  <head>
    <title>Animal Crossing: New Horizons Info</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Animal Crossing: New Horizons Info</h1>
    <h3>Fishes available now</h3>
    <table>
      <thead>
        <th>Icon</th>
        <th>Name</th>
        <th>Shadow Size</th>
      </thead>
      <tbody>
        <% data.map(dat => { %>
          <% if (dat) { %>
            <tr>
              <td><img href="<%= dat.icon %>" alt="img-<%= dat.name %>"/></td>
              <td><%= dat.name %></td>
              <td><%= dat.shadow%></td>
            </tr>
          <% } %>
        <% }) %>
      </tbody>
    </table>

  </body>
</html>
